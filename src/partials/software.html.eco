<% for software in @getSoftware() : %>
	<% if software is "Blender" : %>
		<a href="http://www.blender.org/" target="_blank"><img src="/website-img/icon-blender.svg" alt="<%- software %>" title="<%- software %>"/></a>
	<% else if software is "Gimp" : %>
		<a href="http://www.gimp.org/" target="_blank"><img src="/website-img/icon-gimp.svg" alt="<%- software %>" title="<%- software %>" /></a>
	<% else if software is "Krita" : %>
		<a href="https://krita.org/" target="_blank"><img src="/website-img/icon-krita.svg" alt="<%- software %>" title="<%- software %>" /></a>
	<% else if software is "G'mic" : %>
		<a href="http://gmic.sourceforge.net/" target="_blank"><img src="/website-img/icon-gmic.svg" alt="<%- software %>" title="<%- software %>" /></a>
	<% else if software is "Inkscape" : %>
		<a href="http://www.inkscape.org/" target="_blank"><img src="/website-img/icon-inkscape.svg" alt="<%- software %>" title="<%- software %>" /></a>
	<% else if software is "Mandelbulber" : %>
		<a href="http://www.mandelbulber.com/" target="_blank"><img src="/website-img/icon-mandelbulber.svg" alt="<%- software %>" title="<%- software %>" /></a>
	<% end %>
<% end %>
